<%
if(userData.length!=0){
userData.forEach(function(data){
%>
    <div class="col-md-3 col-sm-6">
    <div class="product-card">
        <a href="/room/<%=data.id_phong_tro%>" style="text-decoration: none;">
            <div class="product-img">
                <img class="img-link" src=<%=data.path_anh%>>
            </div>
            <div class="product-info">
                <div class="info-title">
                    <%=data.phan_loai%> - <%=data.ten_phong%>
                </div>
                <div class="info-category">
                    <span class="phongtro-chungcu"><%=data.phan_loai%></span>
                    <span>·</span>
                    <span class="so-nguoi"><%=data.tong_so_nguoi%> nguoi</span>
                </div>
                <div class="price">
                    <%=data.gia_phong%> đ/1 tháng
                </div>
                <div class="address">
                    <%=data.thanh_pho%> - <%=data.quan_huyen%> - <%=data.phuong_xa%>
                </div>
            </div>
        </a>
    </div>
</div>
<%})} %>

