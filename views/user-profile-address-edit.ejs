<%- include("partials/navbar-user-profile-info", {username: user.ten_nguoi_dung}); -%>
    <div class="container" id="profile-content">
        <div class="row">
            <div class="col-md-2">
                <span style="font-size: 12px;">Xin chào <%=user.ten_nguoi_dung %>,</span>
                <br><br>
                <a href="profile" style="text-decoration: none;">
                    <i class="fas fa-user-circle" style="color: hotpink;font-size: 19px;">&nbsp;</i><span id="catelogy"><b>Quản lý tài khoản</b></span>
                </a>
                <ul style="list-style: none;margin-left: -5px;">
                    <li>
                        <a href="profile-info" style="text-decoration: none;">
                            <span id="ul-li">Thông tin cá nhân</span>
                        </a>
                    </li>
                    <li>
                        <a href="profile-change-password" style="text-decoration: none;">
                            <span id="ul-li">Đổi mật khẩu</span>
                        </a>
                    </li>
                    <li>
                        <a href="profile-address" style="text-decoration: none;">
                            <span id="ul-li" style="color: #1A9CB7;">Địa chỉ</span>
                        </a>
                    </li>
                </ul>
                <a href="#" style="text-decoration: none;">
                    <i class="fas fa-bell" style="color: rgb(255, 30, 0);font-size: 19px;">&nbsp;</i><span id="catelogy"><b>Thông báo</b></span>
                </a>
                <br><br>
                <a href="#" style="text-decoration: none;">
                    <i class="fas fa-clipboard" style="color: rgb(0, 195, 255);font-size: 19px;">&nbsp;</i><span id="catelogy"><b>Bài đã đăng</b></span>
                </a>
                <br><br>
                <a href="post" style="text-decoration: none;">
                    <i class="far fa-edit" style="color: rgb(255, 0, 98);font-size: 19px;">&nbsp;</i><span id="catelogy"><b>Tạo bài viết mới</b></span>
                </a>
            </div>
            <div class="col-md-10">
                <span style="font-size: 17px;"><b>Sửa thông tin địa chỉ</b></span>
                <br><br>
                <form>
                    <div class="content" style="background-color: white; height: 340px;">
                        <div class="row" style="padding:40px;">
                            <div class="col-md-12">
                                <div class="col-md-4">
                                    <span id="title-item-profile"><i>Họ và tên</i></span>
                                    <br>
                                    <input type="text" id="address" class="form-control" placeholder="Nhập họ và tên của bạn" style="border-radius: 0;color: #424242; font-size: small;height: 35px;" required data-bv-notempty-message="Không được để trống">
                                    <br>
                                </div>
                                <div class="col-md-4">
                                    <span id="title-item-profile"><i>Số điện thoại</i></span>
                                    <br>
                                    <input type="text" id="city" class="form-control" style="border-radius: 0;color: #424242; font-size: small;height: 35px;text-align: center;" data-meta="Field" value="<%=user.sdt%>" readonly="" tabindex="-1">
                                    <br>
                                </div>
                                <div class="col-md-4">
                                    <span id="title-item-profile"><i>Tỉnh/Thành phố</i></span>
                                    <br>
                                    <input type="text" id="city" class="form-control" style="border-radius: 0;color: #424242; font-size: small;height: 35px;text-align: center;" data-meta="Field" value="Hà Nội" readonly="" tabindex="-1">
                                    <br>
                                </div>
                            </div>

                            <div class="col-md-12">
                                <div class="col-md-4">
                                    <span id="title-item-profile"><i>Quận/Huyện</i></span>
                                    <br>
                                    <select class="custom-select" id="huyen" name="quan" placeholder="Vui lòng chọn quận/huyện" style="border-radius: 0;color: #424242; font-size: small;height: 35px;width: 100%;" required data-bv-notempty-message="Không được để trống">
                                        <option value="" selected= "selected">Lựa chọn Quận/Huyện</option>
                                      </select>
                                    <br>
                                </div>
                                <div class="col-md-4">
                                    <span id="title-item-profile"><i>Phường/Xã</i></span>
                                    <br>
                                    <select class="custom-select" id="xa" name="xa" style="border-radius: 0;color: #424242; font-size: small;height: 35px;width: 100%;" required data-bv-notempty-message="Không được để trống">
                                        <option value="" selected= "selected">Lựa chọn Phường/Xã</option>
                                      </select>
                                    <br>
                                </div>

                                <div class="col-md-4">
                                    <span id="title-item-profile"><i>Địa chỉ</i></span>
                                    <br>
                                    <input type="text" id="address" class="form-control" placeholder="Vui lòng nhập địa số nhà, tòa nhà, ..." style="border-radius: 0;color: #424242; font-size: small;height: 35px;" required data-bv-notempty-message="Không được để trống">
                                    <br>
                                </div>
                            </div>

                        </div>
                        <div class="row" style="padding:40px;margin-left: 2px;">
                            <div class="col-md-4">
                                <a href="address-edit" style="text-decoration: none;">
                                    <button type="submit" class="btn btn-primary" id="button-edit"><span id = "text-btn"><b>LƯU THÔNG TIN ĐỊA CHỈ</b></span></button>
                                </a>
                            </div>
                        </div>
                    </div>

                </form>
            </div>
        </div>
    </div>
