    <div class="container wrap-content">
        <br>
        <% if(bookingNum != 0){%>
            <h3>Tổng số lịch hẹn cần phê duyệt:
                <%= bookingNum%>
            </h3>
            <%} else{%>
                <br>
                <%}%>
                    <div style="float: right;">
                        <span class="wrap-nav-item">
                <a href = "/await-bookings/1" class = "wrap-a" style="text-decoration: none; " id = "wrap-a-1">
                    Đang chờ được xác nhận
                </a>
            </span>
